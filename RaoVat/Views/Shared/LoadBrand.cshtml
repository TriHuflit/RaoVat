
@model IEnumerable<RaoVat.Models.Brand>
<div style="display: flex;background-color:white;margin-top:12px;">
    @foreach (var item in Model)
    {
        <div style="border-right:1px solid var(--border--color);">
            <a class="cate-link" style="display: block;padding:12px 23px;cursor:pointer;  text-align: center;text-decoration: none; font-size:1.4rem; color: black;">
                <img src="@Url.Content(item.Image)" alt="" style="width:52px;height:52px;  object-fit: cover;">
                <p style="margin:0;margin-top: 8px;">@item.Name</p>
            </a>
        </div>
    }

</div>
